<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated receta -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="receta.presentation.RecetaModelWizardID"
            name="%_UI_RecetaModelWizard_label"
            class="receta.presentation.RecetaModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/RecetaModelFile.gif">
         <description>%_UI_RecetaModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated receta -->
      <editor
            id="receta.presentation.RecetaEditorID"
            name="%_UI_RecetaEditor_label"
            icon="icons/full/obj16/RecetaModelFile.gif"
            extensions="receta"
            class="receta.presentation.RecetaEditor"
            contributorClass="receta.presentation.RecetaActionBarContributor">
      </editor>
   </extension>

</plugin>
